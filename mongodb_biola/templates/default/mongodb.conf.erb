dbpath = /srv/mongodb
port = 27017
bind_ip = <%= @node.ipaddress %>
logpath = /var/log/mongodb/mongodb.log
keyFile = /srv/<%= "replkey_#{@node.chef_environment}" %>
#auth = true ##Implied by keyfile
nohttpinterface = true
replSet = <%= "biola_#{@node.chef_environment}" %>
journal = true
oplogSize = 100